<h2>Show Todo</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @todo.name %>
  </li>

  <li>
    <strong>Priority:</strong>
    <%= @todo.priority %>
  </li>

  <li>
    <strong>user_id:</strong>
    <%= @todo.user_id %>
  </li>

</ul>

<span><%= link "Edit", to: todo_path(@conn, :edit, @todo) %></span> |
<span><%= link "Todo Index", to: todo_path(@conn, :index) %></span>
